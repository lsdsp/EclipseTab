{
    "manifest_version": 3,
    "name": "Eclipse Tab",
    "version": "1.0.0",
    "description": "个人化新标签页浏览器扩展",
    "permissions": [
        "storage",
        "tabs"
    ],
    "host_permissions": [
        "https://suggestqueries.google.com/*",
        "https://www.google.com/*",
        "https://suggestion.baidu.com/*"
    ],
    "chrome_url_overrides": {
        "newtab": "index.html"
    }
}